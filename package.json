{
  "name": "t-minus",
  "version": "1.0.0",
  "description": "",
  "keywords": [
    "countdown"
  ],
  "main": "index.js",
  "engines": {
    "node": "5.1.0"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --config config/webpack.config.js",
    "build:analyze": "NODE_ENV=production webpack --config config/webpack.config.js --json | webpack-bundle-size-analyzer",
    "deploy": "gh-pages-deploy",
    "dev": "NODE_ENV=development npm start",
    "start": "node src/t-minus-server/server",
    "test": "NODE_ENV=test jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dlindahl/t-minus.git"
  },
  "author": "Derek Lindahl",
  "contributors": [
    "Derek Lindahl"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dlindahl/t-minus/issues"
  },
  "homepage": "https://github.com/dlindahl/t-minus#readme",
  "dependencies": {
    "convict": "1.5.0",
    "empty": "0.10.1",
    "hapi": "13.4.1",
    "hapi-webpack-plugin": "1.3.0",
    "keymirror": "0.1.1",
    "lodash": "4.17.2",
    "present": "1.0.0",
    "react": "15.4.1",
    "react-addons-create-fragment": "15.4.1",
    "react-dom": "15.4.1",
    "react-icons": "2.2.1",
    "react-redux": "4.4.6",
    "redux": "3.6.0",
    "redux-thunk": "2.1.0"
  },
  "devDependencies": {
    "babel-core": "6.18.2",
    "babel-jest": "16.0.0",
    "babel-loader": "6.2.8",
    "babel-plugin-lodash": "3.2.10",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "compression-webpack-plugin": "0.3.2",
    "enzyme": "2.6.0",
    "expose-loader": "0.7.1",
    "gh-pages-deploy": "0.4.2",
    "jest": "16.0.2",
    "lolex": "1.5.2",
    "react-addons-test-utils": "15.4.1",
    "react-hot-loader": "3.0.0-beta.6",
    "react-test-renderer": "15.4.1",
    "react-transform-catch-errors": "1.0.2",
    "react-transform-hmr": "1.0.4",
    "redbox-react": "1.3.3",
    "redux-mock-store": "1.2.1",
    "transfer-webpack-plugin": "0.1.4",
    "webpack": "1.13.3",
    "webpack-bundle-size-analyzer": "2.2.0",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.2"
  },
  "gh-pages-deploy": {
    "staticpath": "dist",
    "prep": [
      "build"
    ]
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "config"
    ],
    "setupFiles": [
      "config/jest.config.js"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(test.jsx?|json|node)$",
    "testPathDirs": [
      "src"
    ]
  }
}
