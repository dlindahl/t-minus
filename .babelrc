{
  "env": {
    "development": {
      "plugins": [
        [
          "react-transform",
          {
            "transforms": [
              {
                "transform": "react-transform-hmr",
                "imports": ["react"],
                "locals": ["module"]
              },
              {
                "transform": "react-transform-catch-errors",
                "imports": ["react", "redbox-react"]
              }
            ]
          }
        ]
      ]
    }
  },
  "plugins": [
    "lodash",
    "transform-decorators-legacy",
    "transform-react-constant-elements"
  ],
  "presets": [
    "es2015",
    "react"
  ]
}
